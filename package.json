{
  "name": "servicesdesk",
  "version": "1.0.0",
  "description": "servicesdesk",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.26.0",
    "@sap-cloud-sdk/util": "^3.26.0",
    "@sap/cds": "^8",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/mdk-tools": "^1.12.1",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4",
    "express": "^4",
    "hdb": "^0.19.12",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.8.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-servicesdesk.solicitudesuser": "cds watch --open servicesdesk.solicitudesuser/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-servicesdesk.solicitudesadmin": "cds watch --open servicesdesk.solicitudesadmin/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-servicesdesk.tipossolicitud": "cds watch --open servicesdesk.tipossolicitud/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[development]": {
        "auth": {
          "kind": "mocked",
          "users": {
            "alice": {
              "roles": [
                "User"
              ]
            },
            "bob": {
              "roles": [
                "Admin"
              ]
            }
          }
        }
      },
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      },
      "html5-repo": true,
      "portal": true,
      "destinations": true,
      "workzone": true
    }
  },
  "lcap": true,
  "sapux": [
    "app/servicesdesk.solicitudesuser",
    "app/servicesdesk.solicitudesadmin",
    "app/servicesdesk.tipossolicitud"
  ]
}
